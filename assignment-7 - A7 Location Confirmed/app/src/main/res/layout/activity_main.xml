<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:visibility="visible"
    android:layout_gravity="center"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/hello_world"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="30dp"
        android:gravity="center"
        android:text="Hello World!"
        android:visibility="visible"
        tools:ignore="MissingConstraints"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_margin="20dp"
        android:gravity="center"
        android:visibility="visible"
        tools:ignore="MissingConstraints"
        app:layout_constraintTop_toBottomOf="@id/hello_world"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        >

        <Button
            android:id="@+id/about_me_button"
            android:layout_width="277dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="About Me"
            app:layout_constrainedHeight="true"
            app:layout_constrainedWidth="true"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            />

        <Button
            android:layout_width="277dp"
            android:layout_height="wrap_content"
            android:id="@+id/clicky_clicky_button"
            android:gravity="center"
            android:text="Clicky Clicky"
            app:layout_constrainedHeight="true"
            app:layout_constrainedWidth="true"
            app:layout_constraintTop_toBottomOf="@+id/about_me_button"
            android:layout_margin="20dp"
            tools:ignore="MissingConstraints"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            />

        <Button
            android:layout_width="277dp"
            android:layout_height="wrap_content"
            android:id="@+id/link_collector_button"
            android:gravity="center"
            android:text="Link Collector"
            app:layout_constrainedHeight="true"
            app:layout_constrainedWidth="true"
            app:layout_constraintTop_toBottomOf="@+id/clicky_clicky_button"
            android:layout_margin="20dp"
            tools:ignore="MissingConstraints"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            />

        <Button
            android:layout_width="277dp"
            android:layout_height="wrap_content"
            android:id="@+id/prime_directive_button"
            android:gravity="center"
            android:text="Prime Directive"
            app:layout_constrainedHeight="true"
            app:layout_constrainedWidth="true"
            app:layout_constraintTop_toBottomOf="@+id/link_collector_button"
            android:layout_margin="20dp"
            tools:ignore="MissingConstraints"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            />


        <Button
            android:layout_width="277dp"
            android:layout_height="wrap_content"
            android:id="@+id/location_confirmed_activity_button"
            android:gravity="center"
            android:text="Location Confirmed"
            app:layout_constrainedHeight="true"
            app:layout_constrainedWidth="true"
            app:layout_constraintTop_toBottomOf="@+id/prime_directive_button"
            android:layout_margin="20dp"
            tools:ignore="MissingConstraints"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>